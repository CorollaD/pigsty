PG_EXPORTER_URL='postgres://{{ monitor_username }}:{{ monitor_password }}@localhost:6432/pgbouncer?host=/var/run/postgresql&sslmode=disable'
PG_EXPORTER_CONFIG=/etc/pg_exporter
PG_EXPORTER_LISTEN_ADDRESS=":{{ pgbouncer_exporter_port }}"
PG_EXPORTER_TELEMETRY_PATH="{{ exporter_metric_path }}"
PG_EXPORTER_DISABLE_CACHE=false
PG_EXPORTER_OPTS='--log.level=info --log.format="logger:syslog?appname=pgbouncer_exporter&local=7"'